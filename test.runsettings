<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <RunConfiguration>
    <ResultsDirectory>Reports/TestResults</ResultsDirectory>
  </RunConfiguration>
   <EnvironmentVariables>
      <!-- For debugging selectors, it's recommend to set the following environment variable -->
      <DEBUG>pw:api</DEBUG>
    </EnvironmentVariables>
  <Playwright>
    <BrowserName>Safari</BrowserName>
    <LaunchOptions>
      <Channel>webkit</Channel>
    </LaunchOptions>
  </Playwright>
  <PropertyGroup>
  <PlaywrightPlatform>all</PlaywrightPlatform>
</PropertyGroup>
  
</RunSettings>
